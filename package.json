{
	"name": "leadgeek-extension",
	"version": "1.0.0",
	"private": true,
	"exports": {
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "cross-env NODE_ENV=production npm run build:tailwind && rollup -c",
		"build:tailwind": "postcss public/assets/global.css -o public/assets/index.css",
		"watch:tailwind": "postcss public/assets/global.css -o public/assets/index.css -w",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"dev": "concurrently \"npm run watch:tailwind\" \"rollup -c -w\"",
		"dev:extension": "nodemon --watch src --ext .* --exec \"npm run build\"",
		"start": "sirv public --single",
		"test": "jest",
		"test:watch": "npm test -- --watch",
		"test:coverage": "jest --coverage"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.18.2",
		"@rollup/plugin-alias": "^3.1.9",
		"@rollup/plugin-commonjs": "^17.0.0",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^11.0.0",
		"@rollup/plugin-typescript": "^8.3.1",
		"@testing-library/jest-dom": "^5.16.4",
		"@testing-library/svelte": "^3.0.3",
		"@tsconfig/svelte": "^2.0.0",
		"@types/chrome": "^0.0.179",
		"@types/jest": "^27.5.2",
		"@types/luxon": "^2.0.9",
		"@types/uuid": "^8.3.4",
		"autoprefixer": "^10.4.2",
		"babel-jest": "^27.5.1",
		"concurrently": "^7.0.0",
		"cross-env": "^7.0.3",
		"dotenv": "^16.0.0",
		"jest": "^27.5.1",
		"nodemon": "^2.0.15",
		"postcss": "^8.4.7",
		"postcss-cli": "^9.1.0",
		"rollup": "^2.3.4",
		"rollup-plugin-css-only": "^3.1.0",
		"rollup-plugin-inject-process-env": "^1.3.1",
		"rollup-plugin-livereload": "^2.0.0",
		"rollup-plugin-svelte": "^7.0.0",
		"rollup-plugin-terser": "^7.0.0",
		"svelte": "^3.49.0",
		"svelte-check": "^2.0.0",
		"svelte-jester": "^2.3.2",
		"svelte-preprocess": "^4.10.6",
		"tailwindcss": "^3.0.23",
		"ts-jest": "^27.1.3",
		"ts-node": "^10.8.1",
		"typescript": "^4.7.2",
		"webextension-polyfill": "^0.8.0"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"highlight-words": "^1.2.0",
		"luxon": "^2.3.1",
		"sirv-cli": "^2.0.0",
		"svelte-navigator": "^3.1.5",
		"tailwind-scrollbar": "^1.3.1",
		"uuid": "^8.3.2"
	}
}
