{
	"name": "leadgeek-extension",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"build": "npm run build:tailwind && rollup -c",
		"build:tailwind": "cross-env NODE_ENV=production postcss public/assets/global.css -o public/assets/index.css",
		"watch:tailwind": "postcss public/assets/global.css -o public/assets/index.css -w",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"dev": "concurrently \"rollup -c -w\" \"npm run watch:tailwind\"",
		"dev:extension": "nodemon --watch src --ext .* --exec \"npm run build\"",
		"start": "sirv public --single",
		"test": "jest",
		"test:watch": "npm run test --watch"
	},
	"devDependencies": {
		"@rollup/plugin-alias": "^3.1.9",
		"@rollup/plugin-commonjs": "^17.0.0",
		"@rollup/plugin-node-resolve": "^11.0.0",
		"@rollup/plugin-typescript": "^8.3.1",
		"@testing-library/dom": "^8.11.3",
		"@testing-library/jest-dom": "^5.16.2",
		"@testing-library/svelte": "^3.0.3",
		"@testing-library/user-event": "^13.5.0",
		"@tsconfig/svelte": "^2.0.0",
		"@types/axios": "^0.14.0",
		"@types/chrome": "^0.0.179",
		"@types/jest": "^27.4.1",
		"@types/luxon": "^2.0.9",
		"autoprefixer": "^9.8.8",
		"concurrently": "^7.0.0",
		"cross-env": "^7.0.3",
		"dotenv": "^16.0.0",
		"jest": "^27.5.1",
		"nodemon": "^2.0.15",
		"postcss-cli": "^7.1.2",
		"postcss-load-config": "^3.1.3",
		"rollup": "^2.3.4",
		"rollup-plugin-css-only": "^3.1.0",
		"rollup-plugin-inject-process-env": "^1.3.1",
		"rollup-plugin-livereload": "^2.0.0",
		"rollup-plugin-svelte": "^7.0.0",
		"rollup-plugin-terser": "^7.0.0",
		"svelte": "^3.0.0",
		"svelte-check": "^2.0.0",
		"svelte-preprocess": "^4.0.0",
		"tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17",
		"ts-jest": "^27.1.3",
		"tslib": "^2.3.1",
		"typescript": "^4.6.2",
		"webextension-polyfill": "^0.8.0"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"luxon": "^2.3.1",
		"sirv-cli": "^2.0.0",
		"svelte-navigator": "^3.1.5"
	}
}
